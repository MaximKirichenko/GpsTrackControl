<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.ksgagro.gps.domain">
	<class name="Terminal" table="TERMINAL">
		<id name="id" column="ID">
			<generator class="native" />
		</id>
		<property name="imei" column="IMEI" />
		<property name="name" column="NAME" />
		<property name="soft" column="SOFT" />
		<property name="telephone" column="TELEPHONE_NUMBER" />
		<property name="vehicle" column="VEHICLE" />
	</class>
</hibernate-mapping>