<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.ksgagro.gps.logic.image.persistence.model">
    <class name="com.ksgagro.gps.logic.image.persistence.model.LocatedImageEntity" table="LOCATED_IMAGE">
        <id name="locatedImageId" column="LOCATED_IMAGE_ID">
            <generator class="sequence">
                <param name="sequence">LOCATED_IMAGE_SEQ</param>
            </generator>
        </id>
        <property name="image" column="IMAGE_BYTES"/>
    </class>
</hibernate-mapping>