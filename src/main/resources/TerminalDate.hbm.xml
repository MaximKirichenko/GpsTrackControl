<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.ksgagro.gps.domain">
	<class name="com.ksgagro.gps.domain.TrackEntity" table="DEVICE_DATE">
		<id name="id" column="ID" >
			<generator class="native"></generator>
		</id>
		<property name="imei" column="IMEI" />
		<property name="numberMessage" column="NUMBER_MESSAGE" />
		<property name="messageDate" column="MESSAGE_DATE" />
		<property name="height" column="HEIGHT" />
		<property name="isCorrect" column="CORRECT_COORDINATES" />
		<property name="numberSatellite" column="NUMBER_SATELLITE" />
		<property name="azimuth" column="AZIMUTH" />
		<property name="speed" column="SPEED" />
		<property name="longitude" column="LONGITUDE" />
		<property name="latitude" column="LATITUDE" />
		<property name="leftGasTank" column="LEFT_TANK" />
		<property name="rightGasTank" column="RIGHT_TANK" />
		<property name="CANFuelConsumption" column="CAN_FUEL_CONSUMPTION" />
		<property name="CANFLS" column="CANFLS" />
		<property name="engineSpeed" column="ENGINE_SPEED" />
		<property name="psv" column="PSV" />
		<property name="vBat" column="VBAT" />
	</class>
</hibernate-mapping>